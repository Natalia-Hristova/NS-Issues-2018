<Page shownModally="onShownModally" navigatingTo="navigatingTo">
	<ScrollView>
		<StackLayout>
			<Button text="setToday" tap="setToday" />
			<DatePicker loaded="{{ onDatePickerLoaded }}" date="{{ date }}" verticalAlignment="middle" horizontalAlignment="center" />
		</StackLayout>
	</ScrollView>
</Page>
